@model IEnumerable<TASweb.Models.Terminal>

@{
    ViewBag.Title = "Inicio";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UUID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TicketSerie)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CompanyID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ShiftStatus)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Status)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Location.LocationName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UUID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TicketSerie)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CompanyID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ShiftStatus)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Status)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Location.LocationName)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.TerminalID }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.TerminalID }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.TerminalID })
        </td>
    </tr>
}

</table>
